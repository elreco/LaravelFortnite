{
    "name": "elreco/laravelfortnite",
    "description": "Fortnite API for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre LE CORRE",
            "email": "a.elreco@gmail.com",
            "homepage": "https://tracker.fortnitefrance.eu"
        }
    ],
    "homepage": "https://github.com/elreco/laravelfortnite",
    "keywords": ["Laravel", "Fortnite", "API"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "classmap": [ "src/" ]
    },
    "autoload-dev": {
        "psr-4": {
            "elreco\\LaravelFortnite\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "elreco\\LaravelFortnite\\LaravelFortniteServiceProvider"
            ],
            "aliases": {
                "LaravelFortnite": "elreco\\LaravelFortnite\\Facades\\LaravelFortnite"
            }
        }
    }
}
